<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.jq" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Sunny</name>
    <description>
        De SMART-Wasknijper app
    </description>
    <author email="support@inf1a.com" href="http://inf1a.com">
        INF1A Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="portrait" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-mqtt" spec="~0.3.8" />
    <plugin name="cordova-plugin-qrscanner" spec="^2.5.0" />
	<platform name="android">
        <icon src="res/android/ldpi.png" density="ldpi" />
        <icon src="res/android/mdpi.png" density="mdpi" />
        <icon src="res/android/hdpi.png" density="hdpi" />
        <icon src="res/android/xhdpi.png" density="xhdpi" />
        <icon src="res/android/xxhdpi.png" density="xxhdpi" />
        <icon src="res/android/xxxhdpi.png" density="xxxhdpi" />
    </platform>
</widget>
